{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "SAM Debugger:  demo_external_system_data_injection",
            "invokeTarget": {
                "target": "code",
                "lambdaHandler": "app.lambda_handler",
                "projectRoot": "${workspaceFolder}/dummy_data_source/demo_external_system/data_injection/demo_external_system_data_injection/hello_world"
            },
            "lambda": {
                "runtime": "python3.12",
                "payload": {
                    "json": {
                        "some_key": "some_value"
                    }
                },
                "environmentVariables": {
                    "REGION": "",
                    "SOURCE_S3_BUCKET": "",
                    "SEARCH_PREFIX": "",
                }
            },
            "profile": "default"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}